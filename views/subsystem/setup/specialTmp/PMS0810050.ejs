
<!--我是交通接駁設定-->


<div class="row" id="app-PMS0810050">

    <div class="col-xs-12 col-sm-12">
        <div class="col-sm-11 col-xs-11">
            <div class="row">
                <label id="prg_Id" class="prg_idLbH3 label-fontLg"><%= prg_id %></label>
            </div>
            <div class="row no-margin-right">
                <!-------- tabPage -------->
                <div id="traffic_tab" class="easyui-tabs easyUi-custom1" style="height: 380px; overflow-y: auto;">
                    <div title="<%= __("program")["PMS0810050"]["pick_up"]%>" class="padding-tabs">
                        <div class="col-xs-12 col-sm-12">
                            <div class="row">
                                <table id="pick_up_dg" style="height: 300px;"></table>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div>
                    <div title="<%= __("program")["PMS0810050"]["drop_off"]%>" class="padding-tabs">
                        <div class="col-xs-12 col-sm-12">
                            <div class="row">
                                <table id="drop_off_dg" style="height: 300px;"></table>
                            </div>
                        </div>
                        <div class="clearfix"></div>
                    </div>
                </div>
                <!-------- /.tabPage -------->
            </div>
        </div>

        <div class="col-xs-1 col-sm-1">
            <div class="row">
                <div class="right-menu-co">
                    <ul>
                        <li>
                            <button id="ins-btn" :disabled="saving" class="btn btn-primary btn-white btn-defaultWidth"
                                    @click="doAppendRow"
                                    role="button">
                                <%= __("SystemCommon").Add %>
                            </button>
                        </li>
                        <li>
                            <button id="remove-btn" :disabled="saving"
                                    class="btn btn-danger btn-white btn-defaultWidth"
                                    @click="doRemoveRow();"
                                    role="button">
                                <%= __("SystemCommon").Delete %>
                            </button>
                        </li>
                        <li>
                            <button id="save-btn" :disabled="saving" class="btn btn-primary btn-white btn-defaultWidth"
                                    @click="doSave();"
                                    role="button">
                                <%= __("SystemCommon").Save %>
                            </button>
                        </li>
                    </ul>
                </div>
            </div>
        </div>

    </div>

</div>
<script src="/js/setup/PMS0810050_setup.js"></script>