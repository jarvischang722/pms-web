<div class="col-xs-12">
    <div class="row">
        <label class="prg_idLbH3"><%= prg_id %></label>
    </div>
</div>

<div class="" id="PMS0830070App">

    <!-- 查詢 -->
    <search-comp
            :search-fields="searchFields"
            :search-cond.sync="searchCond"
            :fetch-data="loadDataGridByPrgID">
    </search-comp>

    <div class="col-xs-12 col-sm-12">
        <div class="row">
            <div class="col-xs-11 col-sm-11">
                <div class="row no-margin-right">
                    <table id="PMS0830070_dg" style="height: 300px; display: inline;"></table>
                </div>
            </div>
            <div class="col-xs-1 col-sm-1">
                <div class="row">
                    <div class="right-menu-co">
                        <ul>
                            <li>
                                <button class="btn btn-primary btn-white btn-defaultWidth cashierSetting-btn"
                                        @click="addRoute"
                                        role="button">Add
                                </button>
                            </li>
                            <li>
                                <button class="btn btn-primary btn-white btn-defaultWidth"
                                        @click="delRoutes"
                                        role="button">Delete
                                </button>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <pms0830070-comp
            :editing-row="editingRow"
            :single-data="singleData"
            :single-data-dt="singleDataDt"
            :tmp-c-u-d="tmpCUD"
            :dt2-item-nos-data-list="dt2ItemNosDataList">
    </pms0830070-comp>


    <!--跳出DT的DT視窗-->
    <el-dialog title="服務項目" :visible.sync="dialogServiceItemVisible" size="tiny" :show-close="false"
               :modal-append-to-body="false">
        <el-row>
            <div style="overflow: auto;height: 400px;">
                <table class="css_table" width="100%" id="serviceDt2Dt">
                    <thead class="css_thead">
                        <tr class="css_tr">
                            <th class="ccs_th">勾選</th>
                            <th class="css_th">項目代號</th>
                            <th class="css_th">服務項目</th>
                        </tr>
                    </thead>
                    <tbody class="tbody">

                        <tr class="css_tr" v-for="(item,index) in dt2ShowList">
                            <td class="css_td">
                                <form>
                                    <!--<input type="checkbox" :checked=""-->
                                    <!--:disabled="item.disabled =='true' " name="chk" id="ckItem"-->
                                    <!--@click="chkAdjfolioData(index,item.checking)">-->
                                    <input type="checkbox" :checked="item.checked"
                                           :disabled="item.disabled" :value="item.item_nos" @click="updateCheckData(index)">
                                </form>
                            </td>
                            <td class="css_td">{{item.item_nos}}</td>
                            <td class="css_td">{{item.item_sna}}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div class="space-6"></div>
        </el-row>
        <div>
            <el-row>
                <el-col>
                    <el-button @click="dialogServiceItemVisible=false"><%= __("SystemCommon").Cancel %></el-button>
                    <el-button @click="dialogServiceItemVisible=false"><%= __("SystemCommon").OK %></el-button>
                </el-col>
            </el-row>
        </div>
    </el-dialog>
</div>

<script>
    var gs_prg_id = '<%=prg_id%>';
</script>

<%-include ../vueTemplate/PMS0830070Tmp %>
<%- include ../vueTemplate/searchTmp.ejs %>

<script src="/js/setup/vueCompLib/searchComp.js"></script>
<script src="/js/setup/PMS0830070_setup.js"></script>