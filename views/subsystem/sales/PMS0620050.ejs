<!-- page-content -->
<div>
    <div class="page-header"></div><!-- /.page-header -->
    <!-- 拜訪紀錄(Visit Record) Page-->
    <div class="pageMain">
        <!--查詢條件-->
        <div class="col-xs-12">
            <div class="search-content">
                <div class="search-co">
                    <div class="row">
                        <div class="search-co-content col-xs-12 search-S2">
                            <div class="width-95 searchMain-S2">
                                <div class="grid">
                                    <div class="grid-item">
                                        <label>Co. ID</label>
                                        <select id="" class="input-medium popWindow-s1v2-col2">
                                            <option value="1">23598293</option>
                                            <option value="2">a2322</option>
                                            <option value="3">A0003</option>
                                        </select>
                                    </div>
                                    <div class="grid-item">
                                        <label>Visit Status</label>
                                        <select id="" class="input-medium popWindow-s1v2-col2">
                                            <option value="1">預約拜訪</option>
                                            <option value="2">已拜訪</option>
                                        </select>
                                    </div>
                                    <div class="grid-item">
                                        <label>Sales Rep</label>
                                        <select id="" class="input-medium popWindow-s1v2-col2">
                                            <option value="1">陳瑞克</option>
                                            <option value="2">黃大企</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="row search-footer-btn">
                                <div class="footer-btn-menu-co">
                                    <ul>
                                        <li>
                                            <div class="" data-rel="tooltip" data-placement="bottom"
                                                 title="查詢">
                                                <div class="icon-reStyle icon-co-14"></div>
                                            </div>
                                        </li>
                                        <li>
                                            <div class="" data-rel="tooltip" data-placement="bottom"
                                                 title="清除">
                                                <div class="icon-reStyle icon-co-11"></div>
                                            </div>
                                        </li>
                                        <li>
                                            <div class="icon-moreSearch show-search-detail">
                                                <i class="fa fa-chevron-down"></i>
                                            </div>
                                        </li>
                                    </ul>
                                </div>  <!-- /.footer-btn-menu-co -->
                            </div> <!-- /.search-footer-btn -->
                        </div>

                        <div class="search-co-content col-xs-12 rece-search-detail" style="display: none;">

                            <div class="width-95">
                                <div class="grid">
                                    <div class="grid-item">
                                        <label>Status</label>
                                        <select id="" class="input-medium popWindow-s1v2-col2">
                                            <option value="1">Normal</option>
                                            <option value="2">No</option>
                                        </select>
                                    </div>
                                    <div class="grid-item">
                                        <label>Contract</label>
                                        <select id="" class="input-medium popWindow-s1v2-col2">
                                            <option value="1">已簽約</option>
                                            <option value="2">洽談中</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="grid">
                                    <div class="grid-item">
                                        <label>Co. Name</label>
                                        <select id="" class="input-medium popWindow-s1v2-col4Key">
                                            <option value="1">人人公司</option>
                                            <option value="2">No</option>
                                        </select>
                                    </div>
                                    <div class="searchCheckbox pull-left">
                                        <span class="checkbox">
                                          <label class="checkbox-width">
                                              <input name="form-field-checkbox" type="checkbox" class="ace" checked>
                                              <span class="lbl">Keyword</span>
                                          </label>
                                        </span>
                                    </div>
                                    <div class="clearfix"></div>
                                </div>
                                <div class="grid">
                                    <div class="grid-item">
                                        <label>Co. Type</label>
                                        <select id="" class="input-medium popWindow-s1v2-col2">
                                            <option value="1">簽約公司</option>
                                            <option value="2">非簽約公司</option>
                                        </select>
                                    </div>
                                    <div class="grid-item">
                                        <label>Region</label>
                                        <select id="" class="input-medium popWindow-s1v2-col2">
                                            <option value="1">北區</option>
                                            <option value="2">中區</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="grid">
                                    <div class="grid-item">
                                        <div class="grid-item">
                                            <label>Planned Visit</label>
                                            <input type="date" class="input-medium popWindow-s1v2-col2" />
                                        </div>
                                        <label>Industry</label>
                                        <select id="" class="input-medium popWindow-s1v2-col2">
                                            <option value="1">資訊業</option>
                                            <option value="2">製造業</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="grid">
                                    <div class="grid-item">
                                        <label>Actual Visit</label>
                                        <input type="date" class="input-medium popWindow-s1v2-col2" />
                                    </div>
                                    <div class="grid-item">
                                        <label>Method</label>
                                        <select id="" class="input-medium popWindow-s1v2-col2">
                                            <option value="1">電訪</option>
                                            <option value="2">家訪</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="grid">
                                    <div class="grid-item">
                                        <label>Purpose</label>
                                        <input type="text" class="input-medium popWindow-s1v2-col4Key" />
                                    </div>
                                    <div class="searchCheckbox pull-left">
                                        <span class="checkbox">
                                          <label class="checkbox-width">
                                              <input name="form-field-checkbox" type="checkbox" class="ace" checked>
                                              <span class="lbl">Keyword</span>
                                          </label>
                                        </span>
                                    </div>
                                    <div class="clearfix"></div>
                                </div>
                                <div class="grid">
                                    <div class="grid-item">
                                        <label>Remarks</label>
                                        <input type="text" class="input-medium popWindow-s1v2-col4Key" />
                                    </div>
                                    <div class="searchCheckbox pull-left">
                                        <span class="checkbox">
                                          <label class="checkbox-width">
                                              <input name="form-field-checkbox" type="checkbox" class="ace" checked>
                                              <span class="lbl">Keyword</span>
                                          </label>
                                        </span>
                                    </div>
                                    <div class="clearfix"></div>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div> <!-- row-->
                </div> <!-- /.search-co -->

            </div> <!-- /.row  search-content-->

        </div> <!-- /.col-sm-12 -->
        <div class="clearfix"></div>

        <div class="col-xs-12">
            <div class="col-sm-11 col-xs-11">
                <div class="row no-margin-right">
                    <div class="gridTableHt">
                        <!-- 拜訪紀錄 dataGrid -->
                        <table id="sales_visitRecord-table"></table>
                    </div>
                </div>
            </div>
            <div class="col-xs-1 col-sm-1">
                <div class="row">
                    <div class="right-menu-co">
                        <ul>
                            <li>
                                <button id="ins-btn" class="btn btn-primary btn-white btn-defaultWidth sales_editVisitRecord"
                                        role="button">修改拜訪紀錄</button>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div> <!-- /.col-sm-12 -->
        <div class="clearfix"></div>

    </div>

    <!--  業務 彈出視窗內容(jing)-->
    <%- include sales_module/sales_Btn_1.ejs %>
    <!--  /.End 業務 彈出視窗內容 -->
</div><!-- /.page-content -->

<!--  業務 initial js -->
<%- include sales_module/sales_includeJs.ejs %>
<!--  /.End 業務 initial js  -->

