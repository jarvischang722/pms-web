<div class="container_12 divider">
    <!--toDO-->
    <!--暫時設定高度-->
    <!--window 問題：最後一個<th>會超出範圍，並且看不到-->
    <div class="grid_12 height600 resRoomType-table fixed-table-container cus-roomType-table" style="width: 99% !important;">
        <table class="fancyTable themeTable treeControl" id="resRoomType-table" cellpadding="0" cellspacing="0">
            <thead>
            <tr>
                <th class="ca-headerTitle height-fntThead rp-first-th" rowspan="2">
                    <!--ca-center-->
                    <div class="caSelect">
                        <bac-select class="input-medium ca-select"
                                    v-model="searchData['year']" :data="yearSelectData"
                                    is-qry-src-before="Y" value-field="value" text-field="display"
                                    @update:v-model="val => searchData['year'] = val"
                                    :default-val="searchData['year']">
                        </bac-select>
                        <div class="space-2"></div>
                        <bac-select class="input-medium ca-select"
                                    v-model="searchData['month']" :data="monthSelectData"
                                    is-qry-src-before="Y" value-field="value" text-field="display"
                                    @update:v-model="val => searchData['month'] = val"
                                    :default-val="searchData['month']">
                        </bac-select>
                    </div>
                    <div class="caIcon">
                        <span class="ca-headerIcon">
                            <i class="fa fa-calendar-check-o orange fa-lg" data-rel="tooltip" data-placement="bottom" title="Today"></i>
                        </span>
                    </div>
                    <div class="clearfix"></div>
                </th>
                <!--日期-->
                <th class="dateColor" v-for="field in dateFieldData">{{field}}</th>
            </tr>
            <tr>
                <!--星期-->
                <th v-for="field in dayFieldData" :class="{'holiday-color': field == 'Sat' || field == 'Sun'}">{{field}}</th>
            </tr>
            </thead>
            <tbody>

            <template v-for="(field,fieldIdx) in roomFieldData">
                <template v-for="(roomTyp, roomIdx) in roomTypData[field]">
                    <tr :data-tt-id="5*roomTyp.idx + 1">
                        <td class="middle td-first">{{field}} <span class="roomNumTip">{{roomTyp.room_qnt}}</span></td>
                        <template v-for="emptyRm in roomTyp.emptyRm">
                            <td class="numeric" v-if="emptyRm == ''"> </td>
                            <td class="numeric" v-else> {{ emptyRm }}</td>
                        </template>
                    </tr>
                    <tr :data-tt-id="5*roomTyp.idx + 2" :data-tt-parent-id="5*roomTyp.idx + 1" class="subTree-s">
                        <td class="middle td-first subTree-title">用房數</td>
                        <template v-for="useRm in roomTyp.useRm">
                            <td class="numeric" v-if="useRm == ''"> </td>
                            <td class="numeric" v-else> {{ useRm }}</td>
                        </template>
                    </tr>
                    <tr :data-tt-id="5*roomTyp.idx + 3" :data-tt-parent-id="5*roomTyp.idx + 1" class="subTree-s">
                        <td class="middle td-first subTree-title">非鎖控剩餘數</td>
                        <template v-for="notWrsRm in roomTyp.notWrsRm">
                            <td class="numeric" v-if="notWrsRm == ''"> </td>
                            <td class="numeric" v-else> {{ notWrsRm }}</td>
                        </template>
                    </tr>
                    <tr :data-tt-id="5*roomTyp.idx + 4" :data-tt-parent-id="5*roomTyp.idx + 1" class="subTree-s">
                        <td class="middle td-first subTree-title">鎖控剩餘數</td>
                        <template v-for="wrsRm in roomTyp.wrsRm">
                            <td class="numeric" v-if="wrsRm == ''"> </td>
                            <td class="numeric" v-else> {{ wrsRm }}</td>
                        </template>
                    </tr>
                    <tr :data-tt-id="5*roomTyp.idx + 5" :data-tt-parent-id="5*roomTyp.idx + 1" class="subTree-s">
                        <td class="middle td-first subTree-title">庫存可超訂的剩餘數</td>
                        <template v-for="overBooking in roomTyp.overBooking">
                            <td class="numeric" v-if="overBooking == ''"> </td>
                            <td class="numeric" v-else> {{ overBooking }}</td>
                        </template>
                    </tr>
                </template>
            </template>

            <tr>
                <td class="middle td-first">Total available</td>
                <td class="numeric" v-for="number in totalAvailable.number">{{number}}</td>
            </tr>
            <tr >
                <td class="middle td-first">occupancy</td>
                <td class="numeric" v-for="number in occupancy.number">{{number}}</td>
            </tr>
            <tr >
                <td class="middle td-first">phyOccupancy</td>
                <td class="numeric" v-for="number in phyOccupancy.number">{{number}}</td>
            </tr>
            <tr >
                <td class="middle td-first">phyAvailable</td>
                <td class="numeric" v-for="number in phyAvailable.number">{{number}}</td>
            </tr>


