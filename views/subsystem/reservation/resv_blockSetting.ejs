        $('#blockSettingResult-table').datagrid({
            singleSelect: true,
            collapsible: true,
            // 從json 撈
            url: '/jsonData/blockSettingResult.json',
            method: 'get',
            columns: [[
                {field: 'blockNo', title: 'Block No.', width: 100},
                {field: 'blcokW', title: '', width: 100},
                {field: 'blcokCname', title: 'Agent/Company Name', width: 150},
                {field: 'blockDescription', title: 'Description', width: 500},
            ]]

        });

        // 鎖控 table
        $('#blcokSettingSingle-table').datagrid({
            singleSelect: true,
            collapsible: true,
