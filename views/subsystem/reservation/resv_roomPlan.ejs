<div>
    <div class="page-header"></div><!-- /.page-header -->
    <!-- 依房號訂房(Room Plan) Page-->
    <div id="PMS0110050App">
        <div class="pageMain">
            <div class="col-xs-12">
                <div class="search-content">
                    <div class="search-co">
                        <div class="row">
                            <div class="col-xs-12 search-co-content search-S2">
                                <div class="width-95 searchMain-S2">
                                    <div class="grid">
                                        <div class="grid-item">
                                            <label>房型</label>
                                            <input type="text" class="input-medium medium-c1"/>
                                        </div>
                                        <div class="grid-item">
                                            <label>房號</label>
                                            <input type="text" class="input-medium medium-c1"/>
                                        </div>
                                        <div class="grid-item">
                                            <label>房間特色</label>
                                            <input type="text" class="input-medium medium-c1" placeholder="">
                                        </div>
                                    </div>
                                </div>
                                <div class="row search-footer-btn">
                                    <div class="footer-btn-menu-co">
                                        <ul>
                                            <li>
                                                <div class="" data-rel="tooltip" data-placement="bottom"
                                                     title="查詢">
                                                    <div class="icon-reStyle icon-co-14"></div>
                                                </div>
                                            </li>
                                            <li>
                                                <div class="" data-rel="tooltip" data-placement="bottom"
                                                     title="清除">
                                                    <div class="icon-reStyle icon-co-11"></div>
                                                </div>
                                            </li>

                                            <li>
                                                <div class="icon-moreSearch show-search-detail">
                                                    <i class="fa fa-chevron-down"></i>
                                                </div>
                                            </li>
                                        </ul>
                                    </div>  <!-- /.footer-btn-menu-co -->
                                </div> <!-- /.search-footer-btn -->
                            </div>

                            <div class="search-co-content col-xs-12 search-S2-con rece-search-detail"
                                 style="display: none;">
                                <div class="width-95">
                                    <div class="grid">
                                        <div class="grid-item">
                                            <label>棟別</label>
                                            <select class="input-medium medium-c1">
                                                <option value="A">A棟</option>
                                                <option value="B">B棟</option>
                                                <option value="C">C棟</option>
                                            </select>
                                        </div>
                                        <div class="grid-item">
                                            <label>樓層</label>
                                            <select class="input-medium medium-c1">
                                                <option value="1F">1F</option>
                                                <option value="2F">2F</option>
                                                <option value="3F">3F</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="clearfix"></div>
                                </div>
                            </div>
                        </div> <!-- row-->

                    </div> <!-- /.search-co -->
                </div> <!-- /.row  search-content-->
                <div class="clearfix"></div>
            </div> <!-- /.col-sm-12 -->
            <div class="col-xs-12">
                <div class="col-sm-11 col-xs-11">
                    <div class="row no-margin-right">
                        <div class="plan-btn-group">
                            <ul>
                                <li>
                                    <span class="status_a btn-inlineWidth white" role="button">A</span>
                                </li>
                                <li>
                                    <span class="status_o btn-inlineWidth white" role="button">O</span>
                                </li>
                                <li>
                                    <span class="status_s btn-inlineWidth white" role="button">S</span>
                                </li>
                                <li>
                                    <span class="status_ooo btn-inlineWidth white" role="button">ooo</span>
                                </li>
                            </ul>
                        </div>
                        <!--  依房型訂房 -->
                        <%- include PMS0100000_module/resvRoomPlanTable_1 %>
                        <!--  /.End 依房型訂房 -->
                    </div>
                </div>
                <div class="col-xs-1 col-sm-1">
                    <div class="row">
                        <div class="right-menu-co"> <!--roomPlan-btn 控制離上層高度-->
                            <ul>
                                <li>
                                    <button class="btn btn-primary btn-white btn-defaultWidth width-50 pull-left"
                                            role="button" data-rel="tooltip" data-placement="bottom" title="前7天" @click="changDate(-7)">
                                        <i class="fa fa-angle-left fa-lg"></i>
                                    </button>
                                    <button class="btn btn-primary btn-white btn-defaultWidth width-50 pull-left"
                                            role="button" data-rel="tooltip" data-placement="bottom" title="後7天" @click="changDate(7)">
                                        <i class="fa fa-angle-right fa-lg"></i>
                                    </button>
                                    <div class="clearfix"></div>
                                </li>
                                <li>
                                    <button class="btn btn-primary btn-white btn-defaultWidth width-50 pull-left"
                                            role="button" data-rel="tooltip" data-placement="bottom" title="前14天" @click="changDate(-14)">
                                        <i class="fa fa-angle-double-left fa-lg"></i>
                                    </button>
                                    <button
                                            class="btn btn-primary btn-white btn-defaultWidth width-50 pull-left"
                                            role="button" data-rel="tooltip" data-placement="bottom" title="後14天" @click="changDate(14)">
                                        <i class="fa fa-angle-double-right fa-lg"></i>
                                    </button>
                                    <div class="clearfix"></div>
                                </li>
                                <li>
                                    <button class="btn btn-primary btn-white btn-defaultWidth reservationDialog-1"
                                            role="button">訂房
                                    </button>
                                </li>
                                <li>
                                    <button id="remove-btn"
                                            class="btn btn-primary btn-white btn-defaultWidth reservationDialog-1"
                                            role="button">明細
                                    </button>
                                </li>
                                <li>
                                    <button class="btn btn-skin btn-defaultWidth resv_amenitiesIny"
                                            role="button">Amenities Iny.
                                    </button>
                                </li>

                            </ul>
                        </div>
                    </div>
                </div>
            </div> <!-- /.col-sm-12 -->
            <div class="clearfix"></div>
        </div>
    </div>

    <!--  依房型訂房 彈出視窗內容-->
    <%- include PMS0100000_module/resvRoomTypeBtn_1 %>
    <!--  /.End 彈出視窗內容 -->

    <!--  依房型訂房 彈出視窗內容(ho 新增)-->
    <%- include PMS0100000_module/resvRoomTypeBtn_2 %>
    <!--  /.End 彈出視窗內容(ho 新增) -->


    <!--  依房型訂房 彈出視窗內容(jing 新增2)-->
    <%- include PMS0100000_module/resvRoomTypeBtn_3 %>
    <!--  /.End 彈出視窗內容 -->
</div>


<!--  訂房共用 initial js -->
<%- include PMS0100000_module/resv_includeJs %>
<!--  /.End 訂房共用 initial js  -->


<!-- subSystem 共用 js -->
<%- include ../common/viewsComJs.ejs %>
<!--  /.End subSystem 共用 js  -->

