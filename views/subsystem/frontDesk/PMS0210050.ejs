<!-- 住客查詢 Page -->
<div class="pageMain">
    <div class="col-xs-12">

        <div class="search-content">
            <div class="search-co">
                <div class="row">
                    <div class="search-co-content col-xs-12 search-S2">
                        <div class="width-95 searchMain-S2">
                            <div class="grid">
                                <div class="grid-item">
                                    <label>住客姓名</label>
                                    <input type="text" class="input-medium medium-c1" placeholder="">
                                </div>
                                <div class="grid-item">
                                    <label>訂房公司</label>
                                    <input type="text" class="input-medium medium-c1" placeholder="">
                                </div>
                                <div class="grid-item">
                                    <label>訂房卡號</label>
                                    <input type="text" class="input-medium medium-c1" placeholder="">
                                </div>
                                <div class="grid-item">
                                    <label>房號</label>
                                    <input type="text" class="input-medium medium-c1" placeholder="">
                                </div>
                            </div>
                        </div>
                        <div class="row search-footer-btn">
                            <div class="footer-btn-menu-co">
                                <ul>
                                    <li>
                                        <div class="" data-rel="tooltip" data-placement="bottom"
                                             title="查詢">
                                            <div class="icon-reStyle icon-co-14"></div>
                                        </div>
                                    </li>
                                    <li>
                                        <div class="" data-rel="tooltip" data-placement="bottom"
                                             title="清除">
                                            <div class="icon-reStyle icon-co-11"></div>
                                        </div>
                                    </li>
                                    <li>
                                        <div class="icon-moreSearch show-search-detail">
                                            <i class="fa fa-chevron-down"></i>
                                        </div>
                                    </li>
                                </ul>
                            </div>  <!-- /.footer-btn-menu-co -->
                        </div> <!-- /.search-footer-btn -->
                    </div>

                    <div class="search-co-content col-xs-12 search-S2-con rece-search-detail"
                         style="display: none;">
                        <div class="width-95">
                            <!--1-->
                            <div class="grid">
                                <div class="grid-item">
                                    <label>入住日期</label>
                                    <template>
                                        <el-date-picker v-model="ciDate1" type="date"
                                                        placeholder=""
                                                        class="date-wt input-medium medium-c1">
                                        </el-date-picker>
                                    </template>
                                </div>
                                <div class="grid-item">
                                    <label>公司名稱</label>
                                    <input type="text" class="input-medium medium-c1" placeholder="">
                                </div>
                                <div class="grid-item">
                                    <label>訂房來源</label>
                                    <select class="input-medium medium-c1">
                                        <option value="1">01:電話訂房</option>
                                        <option value="2">02:飯店櫃台</option>
                                        <option value="3">02:官網訂房</option>
                                    </select>
                                </div>
                                <div class="grid-item">
                                    <label>國籍</label>
                                    <select class="input-medium medium-c1">
                                        <option value="TWN">TWN:中華民國台灣</option>
                                        <option value="USA">USA:美國</option>
                                        <option value="JP">JP:日本</option>
                                    </select>
                                </div>
                            </div>
                            <!--2-->
                            <div class="grid">
                                <div class="grid-item">
                                    <label>退房日期</label>
                                    <template>
                                        <el-date-picker v-model="coDate1" type="date"
                                                        placeholder=""
                                                        class="date-wt input-medium medium-c1">
                                        </el-date-picker>
                                    </template>
                                </div>
                                <div class="grid-item">
                                    <label>團號</label>
                                    <input type="text" class="input-medium medium-c1" placeholder="">
                                </div>
                                <div class="grid-item">
                                    <label>市場類別</label>
                                    <select class="input-medium medium-c1">
                                        <option value="1">1.純訂房</option>
                                        <option value="2">2.套裝行程</option>
                                        <option value="3">3.專案促銷</option>
                                        <option value="4">4.票券銷售</option>
                                        <option value="5">5.員工訂房</option>
                                    </select>
                                </div>
                                <div class="grid-item">
                                    <label>車號</label>
                                    <input type="text" class="input-medium medium-c1" placeholder="">
                                </div>
                            </div>
                            <!--3-->
                            <div class="grid">
                                <div class="grid-item">
                                    <label>過濾條件</label>
                                    <select class="input-medium medium-c1">
                                        <option value="1">IN HOUSE+今日C/I</option>
                                        <option value="2">全部</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="clearfix"></div>
                    </div>
                </div> <!-- row-->
            </div> <!-- /.search-co -->
        </div> <!-- /.row  search-content-->
    </div> <!-- /.col-sm-12 -->
    <div class="clearfix"></div>
    <div class="col-xs-12">
        <div class="col-sm-11 col-xs-11">
            <div class="row no-margin-right">
                <div>
                    <!-- 住客查詢 PMS0210050 dataGrid -->
                    <table id="PMS0210050" class="gridTableHt"></table>
                </div>
            </div>
        </div>
        <div class="col-xs-1 col-sm-1">
            <div class="row">
                <div class="right-menu-co">
                    <ul>
                        <li>
                            <button class="btn btn-primary btn-white btn-defaultWidth reservationDialog-1"
                                    role="button">訂房卡
                            </button>
                        </li>
                        <li>
                            <button class="btn btn-primary btn-white btn-defaultWidth foCnt_details"
                                    role="button">住客畫面
                            </button>
                        </li>
                        <li>
                            <button class="btn btn-primary btn-white btn-defaultWidth cashierHistLedger_Detail"
                                    role="button">已結帳處理
                            </button>
                        </li>
                        <li style="display: none;">
                            <button class="btn btn-gray btn-defaultWidth RelatedRms-btn"
                                    role="button">Related RMs
                            </button>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="clearfix"></div>

</div>


