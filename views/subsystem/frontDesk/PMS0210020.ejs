        $(window).on('resize', function () {
            if ($window.width() < 1281) {
                $('.intCounterLeft').removeClass('col-xs-6').addClass('col-xs-12');
            } else {
                $('.intCounterLeft').removeClass('col-xs-12').addClass('col-xs-6');
            }
        })

        $(".show-intCounterRight > .fa-angle-left").click(function () {
            $('.intCounterLeft').removeClass('col-xs-12').addClass('col-xs-6');
            $('.intCounterRight').css('display', 'block');
            $('.fa-angle-left').css('display', 'none');
            $('.fa-angle-right').css('display', 'block');
        });

        $(".show-intCounterRight > .fa-angle-right").click(function () {
            $('.intCounterLeft').removeClass('col-xs-6').addClass('col-xs-12');
            $('.intCounterRight').css('display', 'none');
            $('.fa-angle-right').css('display', 'none');
            $('.fa-angle-left').css('display', 'block');
        });
        /* screen小於1280縮小 END */

